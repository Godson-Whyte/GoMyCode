// ALGORITHM three_nb_add
// VAR
//     num1,num2,num3 : INTEGER;
//     sum : INTEGER:=0;
// BEGIN
//     Read(num1)
//     Read(num2)
//     Read(num3)
//     sum := num1 + num2 + num3;
//     Write(sum)
// END

count := 0; x := 3;
WHILE (count < 3) DO
    count := count + 1;
    y := (1+2*count) % 3;
    SWITCH (y) DO
        default : 
        case 0 : x := x-1; BREAK;
        case 1 : x := x+5;
    END_SWITCH
END_WHILE
